{
  "name": "oh-no-again",
  "version": "0.4.2",
  "description": "Retry and batching utility with abort, timeout, and concurrency support. Inspired by real-world pain.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./oh-no-again.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./oh-no-again.d.ts"
    }
  },
  "files": [
    "dist/",
    "oh-no-again.d.ts",
    "README.md",
    "src/"
  ],
  "keywords": [
    "retry",
    "batching",
    "fetch",
    "abort",
    "concurrency",
    "timeout"
  ],
  "scripts": {
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "test:watch": "vitest"
  },
  "author": "Bouhadjila Hamza",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yupher/oh-no-again"
  },
  "bugs": {
    "url": "https://github.com/yupher/oh-no-again/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "execa": "^9.6.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "bin": {
    "oha": "./bin/oha"
  },
  "dependencies": {
    "minimist": "^1.2.8"
  }
}
